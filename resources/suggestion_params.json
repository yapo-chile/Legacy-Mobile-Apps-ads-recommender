{
	"post_adreply_inmo_apps": {
		"must": ["RegionID","CommuneID","CategoryID"],
		"should": ["Params.EstateType","Params.Rooms"],
		"mustNot":["ListID"],
		"priceRange": [{
			"gte": "7000",
			"lte": "9000"
		}],
		"queryString": [{
			"query": "(private OR pro OR professional)",
			"defaultField": "PublisherType"
		}],
		"decayFunc": [{
			"name": "gauss",
			"field": "ListTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "60d"
		}],
		"queryConf":[
			{
				"sourceAd": "false"
			}
		]
	},
	"post_adreply_inmo": {
		"must": ["RegionID","CommuneID","CategoryID"],
		"should": ["Params.EstateType","Params.Rooms"],
		"mustNot":["ListID"],
		"priceRange": [{
			"gte": "1000",
			"lte": "1000",
			"calculate": "true"
		}],
		"queryString": [{
			"query": "(pro OR professional)",
			"defaultField": "PublisherType"
		}],
		"decayFunc": [{
			"name": "gauss",
			"field": "ListTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "60d"
		}],
		"queryConf":[
			{
				"sourceAd": "true"
			}
		]
	},
	"suggested-ads": {
		"fields": ["Subject", "SubCategory", "Category", "Commune", "Region"],
		"decayFunc": [
			{
			"name": "gauss",
			"field": "ListTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "30d"
			}
		],
		"queryConf":[
			{
				"minTermFreq": "1",
				"minDocFreq": "5",
				"maxQueryTerms": "20",
				"sourceAd": "false"
			}
		]
	},
	"default": {
		"must": ["Category","SubCategory"],
		"should": ["Params.BrandID","Params.ModelID","Params.Regdate","Params.Brand","Params.Model"],
		"mustNot":["ListID"],
		"queryString": [{
			"query": "(pro OR professional)",
			"defaultField": "PublisherType"
		}],
		"decayFunc": [{
			"name": "gauss",
			"field": "ListTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "7d"
		}],
		"queryConf":[
			{
				"minTermFreq": "1",
				"minDocFreq": "5",
				"maxQueryTerms": "20",
				"sourceAd": "true"
			}
		]
	}
}
