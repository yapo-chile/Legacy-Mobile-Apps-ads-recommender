{
	"post_adreply_inmo_apps": {
		"must": ["location.regionId","category.id"],
		"should": ["params.estateType.value","params.rooms.value"],
		"mustNot":["listId"],
		"priceRange": [{
			"gte": "7000",
			"lte": "9000"
		}],
		"queryString": [{
			"query": "(private OR pro OR professional)",
			"defaultField": "publisherType"
		}],
		"decayFunc": [{
			"name": "gauss",
			"field": "listTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "60d"
		}],
		"queryConf":[
			{
				"minTermFreq": "1",
				"minDocFreq": "5",
				"maxQueryTerms": "20",
				"sourceAd": "false"
			}
		]
	},
	"post_adreply_inmo": {
		"must": ["location.regionId","location.communeId","category.id"],
		"should": ["params.estateType.value","params.rooms.value"],
		"mustNot":["listId"],
		"priceRange": [{
			"gte": "1000",
			"lte": "1000",
			"calculate": "true"
		}],
		"queryString": [{
			"query": "(pro OR professional)",
			"defaultField": "publisherType"
		}],
		"decayFunc": [{
			"name": "gauss",
			"field": "listTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "60d"
		}],
		"queryConf":[
			{
				"minTermFreq": "1",
				"minDocFreq": "5",
				"maxQueryTerms": "20",
				"sourceAd": "true"
			}
		]
	},
	"suggested-ads": {
		"fields": ["subject", "category.id.keyword", "category.parentid.keyword", "location.communeId.keyword", "location.regionId.keyword"],
		"decayFunc": [
			{
			"name": "gauss",
			"field": "listTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "30d"
			}
		],
		"queryConf":[
			{
				"minTermFreq": "1",
				"minDocFreq": "5",
				"maxQueryTerms": "20",
				"sourceAd": "false"
			}
		]
	},
	"default": {
		"must": ["category.id", "category.parentId"],
		"should": ["location.regionId.keyword", "params.brand.value.keyword","params.model.value.keyword","params.regdate.value.keyword","params.brand.translate","params.model.translate"],
		"mustNot":["listId"],
		"queryString": [{
			"query": "(pro OR professional)",
			"defaultField": "publisherType"
		}],
		"decayFunc": [{
			"name": "gauss",
			"field": "listTime",
			"origin": "now/1d",
			"offset": "1d",
			"scale": "7d"
		}],
		"queryConf":[
			{
				"minTermFreq": "1",
				"minDocFreq": "5",
				"maxQueryTerms": "20",
				"sourceAd": "true"
			}
		]
	}
}
